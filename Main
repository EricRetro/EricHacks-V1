local function ITHZBPX_fake_script() -- ShowHiderbxm.ShowHide.lua 
	local script = Instance.new('LocalScript', ShowHiderbxm)

	visible=script.Parent.Parent.Visible
	
	script.Parent.MouseButton1Click:connect(function()
		if visible.Value==true then
			for _,obj in pairs(script.Parent.Parent:GetChildren()) do
				if obj:IsA("Frame") then
					obj.Visible=false
					visible.Value=false
				end
			end
		elseif visible.Value==false then
			for _,obj in pairs(script.Parent.Parent:GetChildren()) do
				if obj:IsA("Frame") then
					obj.Visible=true
					visible.Value=true
				end
			end
		end
	end)
end
coroutine.wrap(ITHZBPX_fake_script)()
local function HGAMPWM_fake_script() -- ScreenGui.Main 
	local script = Instance.new('LocalScript', ScreenGui)

	--[[ Only edit lines 39-44! ]]--
	
	GUI = script.Parent.Holder.clicker
	APOS = 0.025
	list = script.Parent.Holder.ID.Frame
	script.Parent.Holder:WaitForChild("ds")
	if not game.Players.LocalPlayer:findFirstChild("Music") then
	music = Instance.new("Sound")
	music.Parent = game.Players.LocalPlayer
	music.Name = "Music"
	music.Looped = true
	music.Volume = 0.5
	mID=Instance.new("NumberValue",music)
	mID.Name = "ID"
	else music = game.Players.LocalPlayer.Music
	end
	usr = game:GetService("UserInputService")
	Window = script.Parent.Holder.ID.Frame
	dp = script.Parent.Holder.ds
	slider = script.Parent.Holder.clicker.percent
	SY = script.Parent.Holder.clicker.percent.Size.Y.Scale
	reader = script.Parent.Holder.clicker.reader 
	drpITM_SPACING = 17
	winY = 1
	local index = 0 
	
	local function select(ID,n)
		music:Stop()
		music.SoundId = "http://www.roblox.com/asset/?id="..ID
		dp.Text = "Loading"
		dp.Text = "Song - "..n
		music.ID.Value = ID
		music:Play()
		dp.BackgroundColor3 = Color3.new(0/255,213/255,255/255)
	end
	
	-- Put ID of song then change the name of what you want displayed on the actual UI.
	musicLIST = {
	[1]={131111368,"Radioactive"};
	[2]={413625451,"Alone"};
	[3]={717250051,"Keep it Mello"};
	[4]={198299546,"Electric Joy Ride"}; 
	[5]={182621515,"Stamp on the Ground"};
	[6]={342954907,"Red Lips"};
	}
	-- Don't go on!
	
	for i,_ in pairs(musicLIST) do
		local drpITM = Instance.new("TextButton",Window)
		drpITM.Name = tostring(i)
		local info = Instance.new("NumberValue",drpITM)
		drpITM.Size = UDim2.new(0,100,0,20)
		drpITM.Position = UDim2.new(0,0,0,index+drpITM_SPACING)
		drpITM.BorderSizePixel = 0
		drpITM.Style = Enum.ButtonStyle.RobloxRoundDefaultButton
		drpITM.TextScaled = false
		drpITM.TextWrapped = true
		drpITM.TextColor3 = Color3.new(1,1,1)
		drpITM.BackgroundTransparency = 1
		drpITM.Font = Enum.Font.Legacy
		info.Value  =_[1]
		drpITM.Text =_[2]
		drpITM.MouseButton1Down:connect(function() select(info.Value,drpITM.Text) end)
		index = drpITM.Position.Y.Offset
		winY = winY+drpITM.Size.Y.Offset
	if music.ID.Value == _[1] then dp.Text = _[2]
		slider:TweenSize(UDim2.new(tonumber(music.Volume),0,SY,0),Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.5,true)
		reader.Text =(math.ceil(music.Volume*100)).."%"
	end
	end
	
	plr = game.Players.LocalPlayer
	mouse = plr:GetMouse()
	sliderSIZE = reader.Parent.Parent.Parent.AbsoluteSize.X
	MAX = 100
	
	GUI.MouseButton1Down:connect(function(x,y)
		local total = slider.AbsolutePosition.X+slider.AbsoluteSize.X
	
	x = x-slider.AbsolutePosition.X
	
	print(x.."/"..total.." Value")
		
		
		if x > MAX then x = MAX end
		slider:TweenSize(UDim2.new(tonumber(x/100),0,SY,0),Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.5,true)
		reader.Text = x.."%"
		music.Volume = x/100
	end)
	
	dp.MouseButton1Down:connect(function()
		if music.SoundId == "" then return end 
		if dp.BackgroundColor3 == Color3.new(0/255,213/255,255/255) then
			music:Pause()
			dp.BackgroundColor3 = Color3.new(1,1,1)
			else music:Play()dp.BackgroundColor3 = Color3.new(0/255,213/255,255/255)
		end
	end)
	
	--[[script.Parent.MouseButton1Down:connect(function()
		if GUI.Position.Y.Scale < APOS then
		GUI:TweenPosition(UDim2.new(0.5,0,APOS,0),Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.5, true)
		else
		GUI:TweenPosition(UDim2.new(0.5,0,-0.025,0),Enum.EasingDirection.In, Enum.EasingStyle.Linear, 0.5, true)
		end
	end)--]]
	
	--if usr.MouseEnabled and usr.KeyboardEnabled then
	print("Computer/Laptop")
	Window.MouseLeave:connect(function()
		Window:TweenSizeAndPosition(UDim2.new(0,Window.AbsoluteSize.X,0,0),UDim2.new(0,(Window.AbsolutePosition.X-Window.Parent.AbsolutePosition.X)/(Window.Parent.AbsolutePosition.X+1),0,(Window.AbsolutePosition.Y-Window.Parent.AbsolutePosition.Y)/(Window.Parent.AbsolutePosition.Y+1)),"Out","Sine",0.25)
	end)
	
	Window.Parent.MouseEnter:connect(function()
		Window:TweenSizeAndPosition(UDim2.new(0,Window.AbsoluteSize.X,0,winY),UDim2.new(0,(Window.AbsolutePosition.X-Window.Parent.AbsolutePosition.X)/(Window.Parent.AbsolutePosition.X+1),0,(Window.AbsolutePosition.Y-Window.Parent.AbsolutePosition.Y)/(Window.Parent.AbsolutePosition.Y+1)),"Out","Sine",0.25)
	end)
	
	
	Window.Parent.FocusLost:connect(function()
		 pcall(function() local asst = game:GetService("MarketplaceService"):GetProductInfo(tonumber(Window.Parent.Text))
		music:Stop()
		music.SoundId="http://www.roblox.com/asset/?id="..tonumber(Window.Parent.Text)
		dp.Text = "Loading"
		if asst.Name:len() > 10 then
		dp.Text = "Song-"..asst.Name:sub(1,11).."..."
		else dp.Text = "Song-"..asst.Name
		end
		music.ID.Value = tonumber(Window.Parent.Text)
		music:Play()
		dp.BackgroundColor3 = Color3.new(0/255,213/255,255/255)
			end)
	end)
end
coroutine.wrap(HGAMPWM_fake_script)()
